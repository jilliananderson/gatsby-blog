{
  "name": "gatsby-blog",
  "private": true,
  "description": "A blog created using Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.27",
    "commitizen": "^4.2.1",
    "cz-customizable": "^6.3.0",
    "cz-customizable-ghooks": "^2.0.0",
    "gatsby": "^2.18.17",
    "gatsby-plugin-emotion": "^4.1.16",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^5.25.1",
    "git-cz": "^4.7.1",
    "husky": "^4.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-kirkham": "^0.16.19"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JillianAndersonSlate/gatsby-blog"
  },
  "bugs": {
    "url": "https://github.com/JillianAndersonSlate/gatsby-blog/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "cz-customizable-ghooks",
      "prepare-commit-msg": "exec < /dev/tty && node_modules/.bin/cz --hook || true"
    }
  }
}
